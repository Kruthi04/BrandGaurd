# BrandGuard Environment Configuration
# Copy this file to .env and fill in your actual API keys

# ── Server ──
HOST=0.0.0.0
PORT=8000
DEBUG=false

# ── Senso API (content evaluation, rules engine, generation) ──
SENSO_GEO_API_KEY=
SENSO_SDK_API_KEY=
SENSO_API_BASE_URL=https://api.senso.ai

# ── Tavily API (web search, crawl, research) ──
TAVILY_API_KEY=

# ── Neo4j (graph database) ──
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=

# ── Yutori API (scouting and browsing) ──
YUTORI_API_KEY=
YUTORI_API_BASE_URL=https://api.yutori.com

# ── Modulate API (voice/audio analysis) ──
MODULATE_API_KEY=
MODULATE_API_BASE_URL=https://api.modulate.ai

# ── OpenAI (agent orchestration LLM) ──
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# ── Frontend (Vite) ──
VITE_API_URL=http://localhost:8000
